package test;

public class Variablestudy {

	public static void main(String[] args) {
		// Dec
		
		int a=10;
		System.out.println("a="+a);
		boolean result;
		
		result=true;
		
		System.out.println("result is"+result);
		
		
		
		 float c;
		 c=74.56f;
		 
		 System.out.println("value of c="+c);
		 
		 double b;
		 
		 b=88.4569525d;
		 
		 System.out.println("value of b="+b);
		 
		 

	}

}
